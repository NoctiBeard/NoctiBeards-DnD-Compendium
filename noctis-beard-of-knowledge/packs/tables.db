e":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"ws7AH5IEGoQGZMba"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>8\n\n<b>Damage Type</b>Psychic\n\n<b>Gem</b>Jade","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"3gDeRFJmDVM8zJ6h"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>9\n\n<b>Damage Type</b>Radiant\n\n<b>Gem</b>Topaz","img":"icons/svg/d20-black.svg","weight":1,"range":[9,9],"drawn":false,"documentCollection":"","documentId":null,"_id":"PJ9SFNwHVOAnvwor"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>10\n\n<b>Damage Type</b>Thunder\n\n<b>Gem</b>Spinel","img":"icons/svg/d20-black.svg","weight":1,"range":[10,10],"drawn":false,"documentCollection":"","documentId":null,"_id":"wOwaijhZRsB4fZgy"}],"formula":"d10","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673813531078,"modifiedTime":1673813531078,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7HKeRreKsmV0kl1x"}
{"name":"Background: Uthgardt Tribe Member: Bond","flags":{"ddbimporter":{"parentName":"Background: Uthgardt Tribe Member","keys":["d6","Bond"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"My family, clan, or tribe is the most important thing in my life, even when they are far from me.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"c2Gc1NFKBnvbmBjJ"},{"flags":{},"type":0,"text":"An injury to the unspoiled wilderness of my home is an injury to me.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"A3ZeHS6hSRcBYfUX"},{"flags":{},"type":0,"text":"I will bring terrible wrath down on the evildoers who destroyed my homeland.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"ztgN4i2ALxivn0iS"},{"flags":{},"type":0,"text":"I am the last of my tribe, and it is up to me to ensure their names enter legend.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"OXuGnTJuYcclsQ9l"},{"flags":{},"type":0,"text":"I suffer awful visions of a coming disaster and will do anything to prevent it.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"TtnmaQ4N3FnpDfeT"},{"flags":{},"type":0,"text":"It is my duty to provide children to sustain my tribe.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"tO4j1WHEuvmoNPJe"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328460,"modifiedTime":1673816328460,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7IRUp98htGLvzfrj"}
{"name":"Background: Sage: Ideal","flags":{"ddbimporter":{"parentName":"Background: Sage","keys":["d6","Ideal"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<b>Knowledge.</b> The path to power and self-improvement is through knowledge. (Neutral)","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"XXbQOamcyhHJd3xZ"},{"flags":{},"type":0,"text":"<b>Beauty.</b> What is beautiful points us beyond itself toward what is true. (Good)","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"gTqtPQ21hRUScbac"},{"flags":{},"type":0,"text":"<b>Logic.</b> Emotions must not cloud our logical thinking. (Lawful)","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"5BBKzhi6kqBSmB3s"},{"flags":{},"type":0,"text":"<b>No Limits.</b> Nothing should fetter the infinite possibility inherent in all existence. (Chaotic)","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"aEbUPdYvdC6EY1zC"},{"flags":{},"type":0,"text":"<b>Power.</b> Knowledge is the path to power and domination. (Evil)","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"pZyJBSEiAyZ7SYLL"},{"flags":{},"type":0,"text":"<b>Self-Improvement.</b> The goal of a life of study is the betterment of oneself. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"b73E92t37x2u8yJ5"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328040,"modifiedTime":1673816328040,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7MgsjrCYcr1WTWBY"}
{"name":"Background: Urchin: Flaw","flags":{"ddbimporter":{"parentName":"Background: Urchin","keys":["d6","Flaw"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"If I’m outnumbered, I will run away from a fight.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"kqVjBWXAlO7J49UL"},{"flags":{},"type":0,"text":"Gold seems like a lot of money to me, and I’ll do just about anything for more of it.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"L7xjzvQJiQSt2zFr"},{"flags":{},"type":0,"text":"I will never fully trust anyone other than myself.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"9fwAywvd7ZrKjikS"},{"flags":{},"type":0,"text":"I’d rather kill someone in their sleep than fight fair.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"sohsXi3F2WmDrku2"},{"flags":{},"type":0,"text":"It’s not stealing if I need it more than someone else.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"Favx3w0hd8bDJ36u"},{"flags":{},"type":0,"text":"People who can’t take care of themselves get what they deserve.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"05n5G4JxbndqJqx7"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328349,"modifiedTime":1673816328349,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7Scc3Mym8NeIEfnd"}
{"name":"Background: House Agent (Phiarlan): House Agent Role","flags":{"ddbimporter":{"parentName":"Background: House Agent (Phiarlan)","keys":["D8","ROLE"],"diceKeys":["D8"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"Acquisition","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"ngR2Pb2HqfZOZBK2"},{"flags":{},"type":0,"text":"@Compendium[dnd5e.rules.0AGfrwZRzSG0vNKb]{Investigation}","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"A7oqkMNBxaHesNDE"},{"flags":{},"type":0,"text":"Research &amp; Development","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"i9sTjzAffcSqKthW"},{"flags":{},"type":0,"text":"Security","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"jey52kWclKKeRUJW"},{"flags":{},"type":0,"text":"@Compendium[dnd5e.rules.0AGfrwZRzSG0vNKb]{Intimidation}","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"j7fiKW7kYl4g65Ro"},{"flags":{},"type":0,"text":"Exploration","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"yNGP58hQCqXKvR52"},{"flags":{},"type":0,"text":"Negotiation","img":"icons/svg/d20-black.svg","weight":1,"range":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"ET4eODaXODQbdYOf"},{"flags":{},"type":0,"text":"Covert Operations","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"SPllyguJP3nAFMja"}],"formula":"","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328775,"modifiedTime":1673816328775,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7fx8ugHUpy6cpFP4"}
{"name":"Background: Sailor - Baldur’s Gate: Personality Trait","flags":{"ddbimporter":{"parentName":"Background: Sailor - Baldur’s Gate","keys":["d8","Personality Trait"],"diceKeys":["d8"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"My friends know they can rely on me, no matter what.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"fzVVYPBwgFtG9dR4"},{"flags":{},"type":0,"text":"I work hard so that I can play hard when the work is done.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"GqSAfcCfw89I6l8A"},{"flags":{},"type":0,"text":"I enjoy sailing into new ports and making new friends over a flagon of ale.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"OMcu9r0KiPPNlZ3V"},{"flags":{},"type":0,"text":"I stretch the truth for the sake of a good story.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"SVzsJpitxeNDX77l"},{"flags":{},"type":0,"text":"To me, a tavern brawl is a nice way to get to know a new city.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"MGsg7gwabpbLhdqG"},{"flags":{},"type":0,"text":"I never pass up a friendly wager.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"z60sbETrS4Kew6sq"},{"flags":{},"type":0,"text":"My language is as foul as an otyugh nest.","img":"icons/svg/d20-black.svg","weight":1,"range":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"VjX1mMM3uwmWHgr8"},{"flags":{},"type":0,"text":"I like a job well done, especially if I can convince someone else to do it.","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"l9UYKUo3PKAewGJc"}],"formula":"d8","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816330492,"modifiedTime":1673816330492,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7ptBnYVRmW2VW8c1"}
{"name":"Background: Orzhov Representative: Non-Orzhov Contacts","flags":{"ddbimporter":{"parentName":"Background: Orzhov Representative","keys":["d10","Contact"],"diceKeys":["d10"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"An Azorius arrester is always snooping into my family’s business transactions.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"2XvBuJYSQchSBWvn"},{"flags":{},"type":0,"text":"A Boros paladin saved my life, to my everlasting shame.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"EfRVnoAqhROTBY9I"},{"flags":{},"type":0,"text":"I know a shopkeeper who is secretly a Dimir agent and tries to make sure that I keep that secret hidden.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"R50UpzYX9nm0ygYV"},{"flags":{},"type":0,"text":"I’m fascinated by the culture of the Golgari kraul, and I have formed a friendship with one of their death priests.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"KpNMUuuBwMaGOxNK"},{"flags":{},"type":0,"text":"A Gruul druid hates me but would never dare to touch me.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"giU2aCDWiCkZ0fsC"},{"flags":{},"type":0,"text":"I know an Izzet engineer who is desperate to pay off a debt accrued by a deceased relative.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"dea9bkXigtt6Mt7n"},{"flags":{},"type":0,"text":"Roll an additional Orzhov contact; you can decide if the contact is an ally or a rival.","img":"icons/svg/d20-black.svg","weight":1,"range":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"wD3ne935vhIpSsJv"},{"flags":{},"type":0,"text":"My childhood friend is now a Rakdos torturer. We still meet for drinks occasionally.","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"0d8dsiKWhMW1gb2E"},{"flags":{},"type":0,"text":"I have the key to a vault where a Selesnya druid is hiding an item of secret shame.","img":"icons/svg/d20-black.svg","weight":1,"range":[9,9],"drawn":false,"documentCollection":"","documentId":null,"_id":"vLcM45gbgW7VsG2I"},{"flags":{},"type":0,"text":"I was married to a Simic bioengineer.","img":"icons/svg/d20-black.svg","weight":1,"range":[10,10],"drawn":false,"documentCollection":"","documentId":null,"_id":"dkNjFZDFr9jh3330"}],"formula":"d10","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816329777,"modifiedTime":1673816329777,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7wp0ciXRweFlAKxK"}
{"name":"Background: Archaeologist: Ideal","flags":{"ddbimporter":{"parentName":"Background: Archaeologist","keys":["d6","Ideal"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Preservation.</span> That artifact belongs in a museum. (Good)","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"FkOdl1DAWMhaojDI"},{"flags":{},"type":0,"text":"<span class=\"Serif-Character-Style_Bold-Serif\">Greed. </span>I won’t risk my life for nothing. I expect some kind of payment. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"7WD5mMfhDbSV0A6V"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Death Wish.</span> Nothing is more exhilarating than a narrow escape from the jaws of death. (Chaotic)","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"cereCX2w3tkCIACn"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Dignity. </span>The dead and their belongings deserve to be treated with respect. (Lawful)","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"Y7jcr8txPxKdVP2A"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Immortality.</span> All my exploring is part of a plan to find the secret of everlasting life. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"bzZBjrY9UKRGB6JM"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Danger. </span>With every great discovery comes grave danger. The two walk hand in hand. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"earq3bXyH5GJvm4S"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328506,"modifiedTime":1673816328506,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"7xcKG8BTGpoxi5Jo"}
{"name":"Background: House Agent (Medani): BOND","flags":{"ddbimporter":{"parentName":"Background: House Agent (Medani)","keys":["D6","BOND"],"diceKeys":["D6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"My house is my family, and I would do anything for my family.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"fmBlgTanJS62bVeI"},{"flags":{},"type":0,"text":"I love someone from another house, but the relationship is forbidden.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"JSTodBi0bfCmoWYk"},{"flags":{},"type":0,"text":"Someone I love was killed by a rival faction within my house, and I will have revenge.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"gr1mm1WoUHolaQdH"},{"flags":{},"type":0,"text":"I don’t care about the house as a whole, but I would do anything for my old mentor.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"mQNXnbaHsjCshAQM"},{"flags":{},"type":0,"text":"My house must evolve, and I’ll lead the evolution.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"RoZ1oTP5yeeJ3YeK"},{"flags":{},"type":0,"text":"I’m determined to impress the leaders of my house, and to become a leader myself.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"WmFJHsEakAXSXH8r"}],"formula":"","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328684,"modifiedTime":1673816328684,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"83rPPf1m95Xy3ffu"}
{"name":"Background: Orzhov Representative: Bonds","flags":{"ddbimporter":{"parentName":"Background: Orzhov Representative","keys":["d6","Bond"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"The unbearable weight of my debt has driven me to desperation.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"s8C008Py14komdne"},{"flags":{},"type":0,"text":"I’m duty-bound to obey the dictates of an ancestor on the Ghost Council.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"zHgV9eCrWVp9PuO7"},{"flags":{},"type":0,"text":"I value my worldly goods more highly than my mortal life.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"ppe9AVaHeAWwFdWZ"},{"flags":{},"type":0,"text":"An oligarch publicly humiliated me, and I will exact revenge on that whole family.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"XMYIhL1TdqDjjGRO"},{"flags":{},"type":0,"text":"My faith in the Obzedat never wavers.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"IG4GNVzpJFWNQh8x"},{"flags":{},"type":0,"text":"I want to prove myself more worthy than an older sibling and thereby ensure that I inherit a greater share of my parents’ wealth.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"VMEhRdvaeOYmxKbc"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816329751,"modifiedTime":1673816329751,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8A3wA7L6d3N8LsKl"}
{"name":"Background: Noble: Personality Trait","flags":{"ddbimporter":{"parentName":"Background: Noble","keys":["d8","Personality Trait"],"diceKeys":["d8"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"My eloquent flattery makes everyone I talk to feel like the most wonderful and important person in the world.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"2hN6Zhx5V0TrGGgS"},{"flags":{},"type":0,"text":"The common folk love me for my kindness and generosity.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"MQyu5ft6YGPwwrjq"},{"flags":{},"type":0,"text":"No one could doubt by looking at my regal bearing that I am a cut above the unwashed masses.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"8ykCQXCmU1xDyAUx"},{"flags":{},"type":0,"text":"I take great pains to always look my best and follow the latest fashions.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"h95WI44osep1NmOU"},{"flags":{},"type":0,"text":"I don’t like to get my hands dirty, and I won’t be caught dead in unsuitable accommodations.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"TymkuWBzgvZE9mIK"},{"flags":{},"type":0,"text":"Despite my noble birth, I do not place myself above other folk. We all have the same blood.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"mW15fZzYrQ1hSKZq"},{"flags":{},"type":0,"text":"My favor, once lost, is lost forever.","img":"icons/svg/d20-black.svg","weight":1,"range":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"ql05LsaYx1G8IdLV"},{"flags":{},"type":0,"text":"If you do me an injury, I will crush you, ruin your name, and salt your fields.","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"cc3nr03hKruY1LZ9"}],"formula":"d8","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328019,"modifiedTime":1673816328019,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8MeXGVrEZGqmGicT"}
{"name":"Background: House Agent (Sivis): IDEAL","flags":{"ddbimporter":{"parentName":"Background: House Agent (Sivis)","keys":["D6","IDEAL"],"diceKeys":["D6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<strong>Common Good.</strong> My house serves a vital function, and its prosperity will help everyone. (Good)","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"YWzDJpHDynGLJzp7"},{"flags":{},"type":0,"text":"<strong>Tradition.</strong> I uphold traditions of my house and bring honor to my family. (Lawful)","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"LRFV2q9z5YMwSO5Y"},{"flags":{},"type":0,"text":"<strong>Innovation.</strong> Abandon old traditions and find better ways to do things. (Chaotic)","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"DgdYVoBOfnPRuIdo"},{"flags":{},"type":0,"text":"<strong>Power.</strong> I want to ensure the prosperity of my house and wield its power myself. (Evil)","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"drcfU6Rp8Mp8sKlb"},{"flags":{},"type":0,"text":"<strong>Discovery.</strong> I want to learn all I can, both for my house and for my own curiosity. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"dngdJmSxMmmtjQOx"},{"flags":{},"type":0,"text":"<strong>Comfort.</strong> I want to ensure that me and mine enjoy the best things in life. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"kFH2o86CwfRgSK9U"}],"formula":"","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328813,"modifiedTime":1673816328813,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8QBpjOlIGJZXAtMs"}
{"name":"Manual of Golems: This tome contains information and incantations necessary to make a particular type of golem. To decipher and use the manual, you must be a spellcaster with at least two 5th-level spell slots. A creature that can't use a manual of golems and attempts to read it takes 6d6 psychic damage.","flags":{"ddbimporter":{"parentName":"Manual of Golems","keys":["d20","Golem","Time","Cost"],"diceKeys":["d20"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<b>[[+ 20]] ([[/r + 20]])</b>1-5\n\n<b>Golem</b>Manual of Clay Golems\n\n<b>Time</b>30 days\n\n<b>Cost</b>65,000 GP","img":"icons/svg/d20-black.svg","weight":1,"range":[1,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"wTdqdw87VS7iF6Sf"},{"flags":{},"type":0,"text":"<b>[[+ 20]] ([[/r + 20]])</b>6-17\n\n<b>Golem</b>Manual of Flesh Golems\n\n<b>Time</b>60 days\n\n<b>Cost</b>50,000 GP","img":"icons/svg/d20-black.svg","weight":1,"range":[6,17],"drawn":false,"documentCollection":"","documentId":null,"_id":"7EDWAC7bYf5rqk9o"},{"flags":{},"type":0,"text":"<b>[[+ 20]] ([[/r + 20]])</b>18\n\n<b>Golem</b>Manual of Iron Golems\n\n<b>Time</b>120 days\n\n<b>Cost</b>100,000 GP","img":"icons/svg/d20-black.svg","weight":1,"range":[18,18],"drawn":false,"documentCollection":"","documentId":null,"_id":"DmzQ7tYwukygXV0W"},{"flags":{},"type":0,"text":"<b>[[+ 20]] ([[/r + 20]])</b>19-20\n\n<b>Golem</b>Manual of Stone Golems\n\n<b>Time</b>90 days\n\n<b>Cost</b>80,000 GP","img":"icons/svg/d20-black.svg","weight":1,"range":[19,20],"drawn":false,"documentCollection":"","documentId":null,"_id":"F7OheEGxlHYWVvbv"}],"formula":"d20","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673813531060,"modifiedTime":1673813531060,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8Y5vauXcPn8JANSb"}
{"name":"Background: Waterdhavian Noble: Bond","flags":{"ddbimporter":{"parentName":"Background: Waterdhavian Noble","keys":["d6","Bond"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"I will face any challenge to win the approval of my family.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"0EzFIHq3ZsUAp81k"},{"flags":{},"type":0,"text":"My house’s alliance with another noble family must be sustained at all costs.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"AqXHR3gJb22zdZyK"},{"flags":{},"type":0,"text":"Nothing is more important than the other members of my family.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"bUX6iRcwWBn2DsWl"},{"flags":{},"type":0,"text":"I am in love with the heir of a family that my family despises.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"YOuoRWf2X2QOIA99"},{"flags":{},"type":0,"text":"My loyalty to my sovereign is unwavering.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"auxoTrjFwzQwIuHA"},{"flags":{},"type":0,"text":"The common folk must see me as a hero of the people.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"a1DEsq0tSEpGfdsy"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816328470,"modifiedTime":1673816328470,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8d0enUnN36KwSbJt"}
{"name":"Rust Bag of Tricks: Rust Bag of Tricks","flags":{"ddbimporter":{"parentName":"Rust Bag of Tricks","keys":["d8","Creature"],"diceKeys":["d8"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"Rat","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"Ch0RnAaMKtwik5O0"},{"flags":{},"type":0,"text":"Owl","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"3eeMjMpH0mCigaxN"},{"flags":{},"type":0,"text":"Mastiff","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"OmNk8ztDPTTcrC0L"},{"flags":{},"type":0,"text":"Goat","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"gs9OIMIBgm1qh8NY"},{"flags":{},"type":0,"text":"Giant goat","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"4ChlIiRH4OPVXpn8"},{"flags":{},"type":0,"text":"Giant boar","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"hXzeDVXDs0SUJErr"},{"flags":{},"type":0,"text":"Lion","img":"icons/svg/d20-black.svg","weight":1,"range":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"Ifuu9gKMcPuTcdFp"},{"flags":{},"type":0,"text":"Brown bear","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"hb8dawwJwQ6NyTGH"}],"formula":"d8","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673813531100,"modifiedTime":1673813531100,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8hAdP0eBpCQXcvZk"}
{"name":"Star Map: Star Map","flags":{"ddbimporter":{"parentName":"Star Map","keys":["d6","Map Form"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"A scroll covered with depictions of constellations","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"JfNlhm9NQGr60EpR"},{"flags":{},"type":0,"text":"A stone tablet with fine holes drilled through it","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"LnCOhZ72m1bmUsZk"},{"flags":{},"type":0,"text":"A speckled owlbear hide, tooled with raised marks","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"m8TlbXG6lRrRsTDd"},{"flags":{},"type":0,"text":"A collection of maps bound in an ebony cover","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"ppT7etnMBxEntEhS"},{"flags":{},"type":0,"text":"A crystal that projects starry patterns when placed before a light","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"6NUGvYMAG2TnW5pL"},{"flags":{},"type":0,"text":"Glass disks that depict constellations","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"nl9MEyRr9Foxt2bJ"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816209881,"modifiedTime":1673816209881,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8pJ5mTVI5PAxWdoF"}
{"name":"Background: Sailor (Revelry Pirate): Ideal","flags":{"ddbimporter":{"parentName":"Background: Sailor (Revelry Pirate)","keys":["d6","Ideal"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Respect. </span>The thing that keeps a ship together is mutual respect between captain and crew. (Good)","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"gjSSl88y5LDvKRNI"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Fairness. </span>We all do the work, so we all share in the rewards. (Lawful)","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"8fZRt65XgB0bXYcF"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Freedom. </span>The sea is freedom — the freedom to go anywhere and do anything. (Chaotic)","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"k8RhVhWXWy7KwjoE"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Mastery.</span> I’m a predator, and the other ships on the sea are my prey. (Evil)","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"VgjOtpOWT7KOnmnK"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">People.</span> I’m committed to my crewmates, not to ideals. (Neutral)","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"ZHZ0Xi9twVzqTb6Q"},{"flags":{},"type":0,"text":"<span class=\"Sans-Serif-Character-Styles_Bold-Sans-Serif\">Aspiration.</span> Someday I’ll own my own ship and chart my own destiny. (Any)","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"Z2F2vUOOLq0trUOQ"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816330060,"modifiedTime":1673816330060,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"8qUQ4nhcJorXTVfZ"}
{"name":"Background: Entertainer - Baldur’s Gate: Bond","flags":{"ddbimporter":{"parentName":"Background: Entertainer - Baldur’s Gate","keys":["d6","Bond"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"My instrument is my most treasured possession, and it reminds me of someone I love.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"taTKt5NwqYP1a9D4"},{"flags":{},"type":0,"text":"Someone stole my precious instrument, and someday I’ll get it back.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"WhYan8dqCGlstZGH"},{"flags":{},"type":0,"text":"I want to be famous, whatever it takes.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"y3w5h3zY02JKqRdc"},{"flags":{},"type":0,"text":"I idolize a hero of the old tales and measure my deeds against that person’s.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"M5SXkcLdLQKbUWda"},{"flags":{},"type":0,"text":"I will do anything to prove myself superior to my hated rival.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"IXaegJRBCrFjWkNh"},{"flags":{},"type":0,"text":"I would do anything for the other members of my old troupe.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"sikSPWnsKCfE51SN"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816330356,"modifiedTime":1673816330356,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"9GsKEKmAcCV29FrJ"}
