g":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"XLjtkCM4gAogDtug"}],"formula":"","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673816329784,"modifiedTime":1673816329784,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"ZtLq8lBbgQJTqL8i"}
