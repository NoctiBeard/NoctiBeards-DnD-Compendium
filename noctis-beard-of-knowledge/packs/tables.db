"},{"flags":{},"type":0,"text":"Halfling, lightfoot","img":"icons/svg/d20-black.svg","weight":1,"range":[61,68],"drawn":false,"documentCollection":"","documentId":null,"_id":"wDeaZt7mzrNkJovp"},{"flags":{},"type":0,"text":"Halfling, stout","img":"icons/svg/d20-black.svg","weight":1,"range":[69,76],"drawn":false,"documentCollection":"","documentId":null,"_id":"AkDJHDU663UUvr27"},{"flags":{},"type":0,"text":"Human","img":"icons/svg/d20-black.svg","weight":1,"range":[77,96],"drawn":false,"documentCollection":"","documentId":null,"_id":"xe2pbzNxUSIALGI9"},{"flags":{},"type":0,"text":"Tiefling","img":"icons/svg/d20-black.svg","weight":1,"range":[97,100],"drawn":false,"documentCollection":"","documentId":null,"_id":"97UYYDywaie1xF0n"}],"formula":"d100","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.2","coreVersion":"10.291","createdTime":1673813488969,"modifiedTime":1673813488969,"lastModifiedBy":"3pKlzol1vXxbcw0b"},"folder":null,"sort":0,"ownership":{"default":0,"3pKlzol1vXxbcw0b":3},"_id":"KFibXqrzotXGQjHA"}
